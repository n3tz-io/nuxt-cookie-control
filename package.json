{
  "author": "Tarik Ermis",
  "contributors": [
    "Dario Ferderber <dario.ferderber@broj42.com>",
    "Jonas Thelemann"
  ],
  "dependencies": {
    "@nuxt/kit": "3.10.2",
    "@sindresorhus/slugify": "^2.2.1",
    "css-vars-ponyfill": "^2.4.8",
    "string-replace-loader": "^3.1.0"
  },
  "description": "Nuxt Cookie Control Module",
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@n3tz-io/nuxt-cookie-control": "link:",
    "@nuxt/module-builder": "0.5.5",
    "@nuxt/schema": "3.10.2",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "nuxt": "3.10.2",
    "prettier": "3.2.5",
    "typescript": "5.3.3",
    "vue": "3.4.19",
    "vue-tsc": "1.8.27",
    "webpack": "5.90.2"
  },
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/types.d.ts"
    },
    "./runtime/*": {
      "import": "./dist/runtime/*.mjs",
      "require": "./dist/runtime/*.mjs",
      "types": "./dist/runtime/*.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "funding": {
    "url": "https://github.com/sponsors/dargmuesli"
  },
  "keywords": [
    "nuxt cookie control",
    "nuxt gdpr",
    "nuxt cookie consent",
    "nuxt gdpr consent"
  ],
  "license": "MIT",
  "main": "./dist/module.cjs",
  "maintainers": [
    "Tarik Ermis"
  ],
  "name": "@n3tz-io/nuxt-cookie-control",
  "packageManager": "pnpm@8.15.3",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/n3tz-io/nuxt-cookie-control",
  "resolutions": {
    "@nuxt/kit": "3.10.2"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "lint": "eslint --ext .js,.ts,.vue . && nuxt typecheck",
    "lint:fix": "eslint --ext .js,.ts,.vue --fix . && nuxt typecheck --fix",
    "prepack": "pnpm build",
    "prepare": "husky"
  },
  "type": "module",
  "types": "./dist/module.d.ts",
  "version": "7.3.3"
}
